{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/packages/core/third_party/ultralib/include/gcc",
            "${workspaceFolder}/packages/core/third_party/ultralib/include",
            "${workspaceFolder}/packages/core/build/include",
            "${workspaceFolder}/packages/core/include"
        ],
        "myDefines": [
            "_LANGUAGE_C=1",
            "F3DEX_GBI_2 = 1",
            "DEBUG = 1"
        ]
    },
    "configurations": [
        {
            "name": "Native",
        },
        {
            "name": "OoT",
            "includePath": [
                "${myIncludePath}"
            ],
            "defines": [
                "${myDefines}",
                "GAME_OOT=1"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerPath": ""
        },
        {
            "name": "MM",
            "includePath": [
                "${myIncludePath}"
            ],
            "defines": [
                "${myDefines}",
                "GAME_MM=1"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerPath": ""
        }
    ],
    "version": 4
}
