#include <combo/defs.h>

SECTIONS
{

  gComboCtxRead   = COMBO_CTX_ADDR_READ;
  gComboCtxWrite  = COMBO_CTX_ADDR_WRITE;

#ifdef GAME_OOT
# include "link_oot.in"
#endif

#ifdef GAME_MM
# include "link_mm.in"
#endif

  . = PAYLOAD_RAM;
  .text : {
    SORT(*)(.text*);
    SORT(*)(.data*);
    SORT(*)(.rodata*);
    SORT(*)(.bss*);
  }

  .MIPS.abiflags : {
    SORT(*)(.MIPS.abiflags*);
  }

  . = 0x80800000;
  .patch : {
    SORT(*)(.patch);
  }
  /DISCARD/ : { *(.*); }
}
